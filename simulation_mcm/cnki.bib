@article{__2014,
	title = {中央空调控制特点与节能技术},
	volume = {5},
	issn = {1674-8646},
	url = {http://kns.cnki.net/kns/detail/detail.aspx?QueryID=18&CurRec=3&recid=&FileName=HELJ201407247&DbName=CJFD2014&DbCode=CJFQ&yx=&pr=&URLID=},
	abstract = {中央空调是温度与湿度调节的重要系统,在企业生产、第三产业服务和公众生活中具有重要的地位。本研究从中央空调控制的角度展开了分析,阐述了中央空调的基本控制原理,描述了中央空调控制的基本特点,强调了中央空调实现节能控制的基本方法,提出了中央空调节能控制过程中应该注意的问题,希望能够使中央空调的运行和控制在有效工作的条件下更加节能,为生产、服务和生活创造更为良好的环境与空间。},
	language = {中文;},
	number = {07},
	urldate = {2018-11-15},
	journal = {黑龙江科学},
	author = {成, 路},
	year = {2014},
	keywords = {基本特点, 节能控制, 控制方法, 控制原理, 在线调试, 中央空调},
	pages = {275}
}

